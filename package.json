    "fs-extra": "^11.3.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "marked": "^17.0.1",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prisma": "^7.3.0",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "webpack": "^5.104.1"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts",
    "seed:adrs": "ts-node scripts/seed-adrs.ts",
    "registry:generate": "ts-node scripts/generate-interface-registry.ts",
    "rollout:all": "ts-node scripts/rollout-standards.ts"
