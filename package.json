{
  "name": "dashboard",
  "version": "1.12.1",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "dev:turbo": "next dev --turbo",
    "prebuild": "node scripts/update-build.js && node scripts/optimize-images.js",
    "verify": "node scripts/verify-deployment.js",
    "build": "npx prisma generate && next build",
    "refresh-data": "npx -y dotenv-cli -e .env.local -- npx tsx scripts/fetch-github-repos.ts",
    "sync:local-files": "npx -y dotenv-cli -e .env.local -- npx tsx scripts/local-files-to-db.ts ../",
    "maintain:verify": "npx -y dotenv-cli -e .env.local -- node scripts/verify-tasks.js",
    "maintain:node": "node scripts/standardize-node.js ../",
    "maintain:audit": "npx -y dotenv-cli -e .env.local -- node scripts/audit-ecosystem.js ../",
    "start": "next start",
    "lint": "eslint",
    "enrich": "npx tsx scripts/enrich-portfolio.ts",
    "sync-repos": "npx tsx scripts/fetch-github-repos.ts",
    "seed:providers": "npx -y dotenv-cli -e .env.local -- node scripts/seed-providers.js",
    "link:providers": "npx -y dotenv-cli -e .env.local -- npx tsx scripts/link-providers.ts",
    "portfolio:refresh": "npx -y dotenv-cli -e .env.local -- npx tsx scripts/intelligence/pipeline.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@sparticuz/chromium": "^143.0.0",
    "@supabase/supabase-js": "^2.49.4",
    "@tremor/react": "^3.18.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dagre": "^0.8.5",
    "dotenv": "^17.2.3",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.555.0",
    "next": "^16.0.10",
    "next-auth": "^4.24.7",
    "next-themes": "^0.4.6",
    "openai": "^6.9.1",
    "puppeteer": "^24.31.0",
    "puppeteer-core": "^24.33.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "reactflow": "^11.11.4",
    "remark-gfm": "^4.0.1",
    "sqlite3": "^5.1.7",
    "swagger-ui-react": "^5.31.0",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@prisma/client": "^5.22.0",
    "@types/dagre": "^0.7.53",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/swagger-ui-react": "^5.18.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9",
    "eslint-config-next": "16.0.5",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.18",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "webpack": "^5.104.1"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts",
    "seed:adrs": "ts-node scripts/seed-adrs.ts",
    "registry:generate": "ts-node scripts/generate-interface-registry.ts",
    "rollout:all": "ts-node scripts/rollout-standards.ts"
  },
  "engines": {
    "node": ">=20.9.0"
  }
}
