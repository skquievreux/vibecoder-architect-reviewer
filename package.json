{
  "name": "dashboard",
  "version": "2.17.7",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "dev:turbo": "next dev --turbo",
    "prebuild": "node scripts/update-build.js && node scripts/optimize-images.js",
    "verify": "node scripts/verify-deployment.js",
    "build": "node scripts/build-version-info.js && pnpm exec prisma generate && next build",
    "refresh-data": "pnpm dlx dotenv-cli -e .env.local -- pnpm dlx tsx src/lib/github-sync.ts",
    "sync:local-files": "pnpm dlx dotenv-cli -e .env.local -- pnpm dlx tsx scripts/local-files-to-db.ts ../",
    "maintain:verify": "pnpm dlx dotenv-cli -e .env.local -- node scripts/verify-tasks.js",
    "maintain:node": "node scripts/standardize-node.js ../",
    "maintain:audit": "pnpm dlx dotenv-cli -e .env.local -- node scripts/audit-ecosystem.js ../",
    "start": "next start",
    "lint": "eslint",
    "enrich": "pnpm dlx tsx scripts/enrich-portfolio.ts",
    "sync-repos": "pnpm dlx tsx src/lib/github-sync.ts",
    "seed:providers": "pnpm dlx dotenv-cli -e .env.local -- node scripts/seed-providers.js",
    "link:providers": "pnpm dlx dotenv-cli -e .env.local -- pnpm dlx tsx scripts/link-providers.ts",
    "portfolio:refresh": "pnpm dlx dotenv-cli -e .env.local -- pnpm dlx tsx scripts/intelligence/pipeline.ts"
  },
  "dependencies": {
