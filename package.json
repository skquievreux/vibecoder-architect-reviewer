    "@types/uuid": "^11.0.0",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "csv-parse": "^6.1.0",
    "dagre": "^0.8.5",
    "dotenv": "^17.2.3",
    "gray-matter": "^4.0.3",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.1.0",
    "lucide-react": "^0.562.0",
    "next": "^16.1.3",
    "next-auth": "^4.24.7",
    "next-themes": "^0.4.6",
    "openai": "^6.16.0",
    "puppeteer": "^24.31.0",
    "puppeteer-core": "^24.33.0",
    "react": "^19.2.3",
